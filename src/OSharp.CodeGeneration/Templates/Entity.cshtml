using System;
using System.Collections.Generic;
using System.ComponentModel;

namespace @(Model.Module.Namespace).Entities
{
    /// <summary>
    /// 实体类：@(Model.Display)信息
    /// </summary>
    [Description("@(Model.Display)信息")]
    public class @Model.Name : EntityBase<@(keyType)>
    {
@foreach (var prop in Model.Properties)
{
    <text>
        /// &lt;summary&gt;
        /// 获取或设置 @prop.Display
        /// &lt;/summary&gt;
        [DisplayName("@prop.Display")]
        public string @prop.Name { get; set; }
    </text>
}

    }
}
